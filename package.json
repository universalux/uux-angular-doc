{
  "name": "uux-angular-doc",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:uux-angular-doc": "node dist/uux-angular-doc/server/server.mjs",
    "prepare": "husky",
    "test:husky": "ng test --watch=false --browsers=ChromeHeadless",
    "server": "ng build & node dist/uux-angular-doc/server/server.mjs",
    "share": "ng serve --host 0.0.0.0 --port 4200",
    "build:prod": "ng build --output-path docs --base-href /",
    "doc": "http-server docs -p 4201 -o"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/platform-server": "^20.1.0",
    "@angular/router": "^20.1.0",
    "@angular/ssr": "^20.1.1",
    "dedent": "^1.7.0",
    "express": "^5.1.0",
    "highlight.js": "^11.11.1",
    "ng-content-card": "^20.0.0",
    "ng-content-carousel": "^20.0.2",
    "ng-expand": "^20.0.0",
    "ng-expand-group": "^20.0.1",
    "ng-hero-carousel": "^20.0.5",
    "ng-link-button": "^20.0.5",
    "ng-menu-toggle": "^20.0.1",
    "ng-ratings": "^20.0.0",
    "ng-scroll-nav": "^20.0.0",
    "ng-simple-button": "^20.0.2",
    "ng-theme-toggle": "^20.0.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uninstall": "^0.0.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.1",
    "@angular/cli": "^20.1.1",
    "@angular/compiler-cli": "^20.1.0",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2"
  }
}
